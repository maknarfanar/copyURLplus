{

	"manifest_version" : 2,
	"name": "copyURLplus",
	"version" : "0.1",
	"description" : "This is a description of the extension.",
	
	"permissions" : [
		"activeTab"
	],

	"icons": { 
		"16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png" 
	},
		  
	"background" : {
		"scripts" : [
			"background.js",
			"copyURL.js"
		]
	},
	
	"browser_action" : {
		"default_icon" : "icon.png"	
	}
}